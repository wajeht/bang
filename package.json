{
	"name": "bang",
	"version": "0.0.1",
	"description": "centralized searching & synchronized cross-platform bookmarking system",
	"main": "./src/server.ts",
	"scripts": {
		"dev": "tsx watch --clear-screen=false ./src/server.ts",
		"update:deps": " npx npm-check -u",
		"check:deps": "npx depcheck",
		"format:check": "prettier --check .",
		"format": "prettier --write .",
		"lint:check": "eslint .",
		"lint": "eslint --fix .",
		"test": "vitest"
	},
	"keywords": [
		"search",
		"bookmark",
		"sync",
		"wajeht"
	],
	"author": "wajeht",
	"license": "MIT",
	"dependencies": {
		"express": "^4.18.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^3.0.0",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"vitest": "^0.33.0"
	}
}
