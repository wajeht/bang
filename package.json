{
  "name": "bang",
  "version": "0.0.1",
  "private": "true",
  "description": "DuckDuckGo's !Bangs, but on steroids",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others --names \"GO,TAILWIND\" -c \"bgGreen.black,bgCyan.black\" \"npm run dev:go\" \"npm run dev:tailwind\"",
    "dev:go": "$(go env GOPATH)/bin/air -c ./.air.toml",
    "dev:tailwind": "tailwindcss build -o public/css/styles.css --watch"
  },
  "keywords": [
    "bang"
  ],
  "author": "wajeht",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.0.1",
    "tailwindcss": "^3.4.13"
  }
}
